{
  "name": "@tableau/api-external-contract-js",
  "version": "1.225.0",
  "description": "External Interfaces for Extensions and Embedding API",
  "repository": "https://gitlab.tableausoftware.com/extensibility/api-external-js.git",
  "main": "./lib/src/ExternalContract.js",
  "types": "./lib/src/ExternalContract.d.ts",
  "license": "UNLICENSED",
  "scripts": {
    "build": "cd ../.. && yarn build",
    "dev": "cd ../.. && yarn dev",
    ":build": "run-s compile docs-extensions docs-embedding docs-embedding-preview",
    "compile": "module-dev-scripts compile",
    "compile-watch": "node_modules/.bin/concurrently --kill-others \"module-dev-scripts compile -w\"  \"module-dev-scripts compile\"",
    "docs-extensions": "typedoc --options typedoc-docs-extensions.json src/ExternalContract/Extensions src/ExternalContract/Shared src/ExtensionsApiExternalContract.ts",
    "docs-embedding": "typedoc --options typedoc-docs-embedding.json src/ExternalContract/Embedding src/ExternalContract/Shared src/EmbeddingApiExternalContract.ts",
    "docs-embedding-preview": "showdown makehtml -i embedding-preview/getting-started-v3.md -o embedding-preview/getting-started-v3.html --ghCompatibleHeaderId --ghCodeBlocks --completeHTMLDocument --tables",
    "lint": "npx eslint --ext ts --ext tsx src/ test/",
    "lint-fix": "npx eslint --ext ts --ext tsx src/ test/ --fix",
    ":check-if-types-published": "cd ../.. && yarn :check-if-types-published",
    ":check-if-published": "yarn gitlab-tools checkIfVersionPublished -d .",
    ":publish-if-new": "yarn gitlab-tools publish -d .",
    ":publish:bump": "echo \"bumping external-contract\" && yarn version --minor --no-git-tag-version",
    "yarn-install": "yarn install --frozen-lockfile --mutex network"
  },
  "devDependencies": {
    "@tableau/gitlab-tools": "^4.1.0",
    "@tableau/module-dev-scripts": "^31.6.0",
    "concurrently": "^6.0.0",
    "eslint": "8.6.0",
    "showdown": "^1.9.1",
    "typedoc": "^0.16.0",
    "typescript": "3.7.2"
  },
  "publishConfig": {
    "registry": "https://artifactory.prod.tableautools.com/artifactory/api/npm/tab-npm-local"
  }
}
