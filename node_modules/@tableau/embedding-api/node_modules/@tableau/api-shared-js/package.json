{
  "name": "@tableau/api-shared-js",
  "version": "1.280.0",
  "description": "Shared code for Extensions and Embedding API",
  "repository": "https://gitlab.tableausoftware.com/extensibility/api-external-js.git",
  "main": "./lib/src/ApiShared.js",
  "types": "./lib/src/ApiShared.d.ts",
  "license": "UNLICENSED",
  "scripts": {
    "build": "cd ../.. && yarn build",
    "dev": "cd ../.. && yarn dev",
    ":build": "run-s compile",
    ":test": "run-s coverage",
    ":check-if-published": "yarn gitlab-tools checkIfVersionPublished -d .",
    ":publish-if-new": "yarn gitlab-tools publish -d .",
    ":publish:bump": "echo \"bumping shared\" && yarn version --minor --no-git-tag-version",
    "compile": "module-dev-scripts compile",
    "compile-watch": "node_modules/.bin/concurrently --kill-others \"module-dev-scripts compile -w\"  \"module-dev-scripts compile\"",
    "coverage": "module-dev-scripts test --coverage",
    "lint": "npx eslint --ext ts --ext tsx src/ test/",
    "lint-fix": "npx eslint --ext ts --ext tsx src/ test/ --fix",
    "test": "module-dev-scripts test --browsers browserstack_chrome,browserstack_firefox,browserstack_ie,browserstack_safari,browserstack_edge",
    "test-dev": "module-dev-scripts test --dev",
    "yarn-install": "yarn install --frozen-lockfile --mutex network"
  },
  "dependencies": {
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@tableau/api-external-contract-js": "1.225.0",
    "@tableau/api-internal-contract-js": "1.191.0",
    "@tableau/gitlab-tools": "^4.1.0",
    "@tableau/module-dev-scripts": "^31.6.0",
    "concurrently": "^6.0.0",
    "eslint": "8.6.0",
    "typescript": "3.7.2"
  },
  "publishConfig": {
    "registry": "https://artifactory.prod.tableautools.com/artifactory/api/npm/tab-npm-local"
  }
}
