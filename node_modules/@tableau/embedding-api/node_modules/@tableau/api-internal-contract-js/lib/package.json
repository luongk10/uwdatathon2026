{
    "name": "@tableau/api-internal-contract-js",
    "version": "1.191.0",
    "description": "The API contract between Tableau and our external client.",
    "repository": "https://gitlab.tableausoftware.com/browser-clients/api-internal-contract-js.git",
    "license": "UNLICENSED",
    "main": "./lib/src/JsApiInternalContract.js",
    "types": "./lib/src/JsApiInternalContract.d.ts",
    "scripts": {
        "build": "cd ../.. && yarn build",
        "dev": "cd ../.. && yarn dev",
        ":build": "yarn compile",
        ":test": "yarn coverage",
        ":check-if-published": "yarn gitlab-tools checkIfVersionPublished -d .",
        ":publish-if-new": "yarn gitlab-tools publish -d .",
        ":publish:bump": "echo \"bumping internal-contract\" && yarn version --minor --no-git-tag-version",
        "compile": "module-dev-scripts compile",
        "compile-watch": "module-dev-scripts compile -w",
        "coverage": "module-dev-scripts test --coverage",
        "lint": "npx eslint --ext ts --ext tsx src/ test/",
        "lint-fix": "npx eslint --ext ts --ext tsx src/ test/ --fix",
        "test": "module-dev-scripts test --browsers browserstack_chrome,browserstack_firefox,browserstack_ie,browserstack_safari,browserstack_edge",
        "test-dev": "module-dev-scripts test --dev",
        "yarn-install": "yarn install --frozen-lockfile --mutex network"
    },
    "resolutions": {
        "@tableau/module-dev-scripts/**/chokidar": "^3.0.1",
        "**/yargs/y18n": "^5"
    },
    "dependencies": {
        "guid": "0.0.12"
    },
    "devDependencies": {
        "@tableau/api-internal-contract-js-v0": "npm:@tableau/api-internal-contract-js@^0",
        "@tableau/gitlab-tools": "^4.1.0",
        "@tableau/module-dev-scripts": "^31.6.0",
        "colors": "1.4.0",
        "eslint": "8.6.0",
        "typescript": "3.7.2"
    },
    "peerDependencies": {},
    "publishConfig": {
        "registry": "https://artifactory.prod.tableautools.com/artifactory/api/npm/tab-npm-local"
    }
}
